{
  "name": "web-projects",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/web/projects",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/web/projects",
        "outputPath": "dist/apps/web/projects"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/web/projects"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "web-projects:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "web-projects:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "web-projects:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "web-projects:build:production"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/web/projects/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
