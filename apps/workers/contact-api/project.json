{
  "name": "worker-contact-api",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/workers/contact-api/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/workers/contact-api",
        "main": "apps/workers/contact-api/src/worker.ts",
        "tsConfig": "apps/workers/contact-api/tsconfig.json",
        "format": ["esm"],
        "assets": [
          {
            "input": "./apps/workers/contact-api",
            "glob": "**/*.toml",
            "output": "."
          }
        ]
      },
      "configurations": {
        "development": {
          "esbuildOptions": {
            "sourcemap": true,
            "bundle": false,
            "outdir": "dist/apps/workers/contact-api",
            "outExtension": {
              ".js": ".mjs"
            }
          }
        },
        "production": {
          "esbuildOptions": {
            "sourcemap": false,
            "bundle": true,
            "outdir": "dist/apps/workers/contact-api",
            "outExtension": {
              ".js": ".mjs"
            }
          }
        }
      }
    },
    "serve": {
      "executor": "@open-system/executors-typescript:cloudflare-worker-serve",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "worker-contact-api:serve"
      },
      "configurations": {
        "development": {
          "buildTarget": "worker-contact-api:serve:development"
        },
        "production": {
          "buildTarget": "worker-contact-api:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/workers/contact-api/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/workers/contact-api/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "deploy": {
      "executor": "@open-system/executors-typescript:cloudflare-worker-deploy",
      "defaultConfiguration": "production",
      "tsconfig": "apps/workers/contact-api/tsconfig.json",
      "options": {
        "buildTarget": "worker-contact-api:deploy"
      },
      "production": {
        "buildTarget": "worker-contact-api:deploy:production"
      }
    },
    "semantic-release": {
      "executor": "@theunderscorer/nx-semantic-release:semantic-release",
      "options": {
        "github": true,
        "npm": false,
        "changelog": true,
        "tagFormat": "worker-contact-api-v${VERSION}"
      }
    }
  },
  "tags": []
}
