{
  "name": "worker-contact-submit",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/workers/contact-submit/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@open-system/executors-typescript:cloudflare-worker-build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "platform": "node",
        "outputPath": "dist/apps/workers/contact-submit",
        "format": ["cjs"],
        "bundle": false,
        "main": "apps/workers/contact-submit/src/index.ts",
        "tsConfig": "apps/workers/contact-submit/tsconfig.json",
        "assets": ["apps/workers/contact-submit/src/assets"],
        "generatePackageJson": true,
        "esbuildOptions": {
          "sourcemap": true,
          "outExtension": {
            ".js": ".js"
          }
        }
      },
      "configurations": {
        "development": {},
        "production": {
          "esbuildOptions": {
            "sourcemap": false,
            "outExtension": {
              ".js": ".js"
            }
          }
        }
      }
    },
    "serve": {
      "executor": "@open-system/executors-typescript:cloudflare-worker-serve",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "worker-contact-submit:serve"
      },
      "configurations": {
        "development": {
          "buildTarget": "worker-contact-submit:serve:development"
        },
        "production": {
          "buildTarget": "worker-contact-submit:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/workers/contact-submit/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/workers/contact-submit/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "deploy": {
      "executor": "@open-system/executors-typescript:cloudflare-worker-deploy",
      "defaultConfiguration": "production",
      "name": "worker-contact-submit",
      "tsconfig": "apps/workers/contact-submit/tsconfig.json",
      "minify": true,
      "no-bundle": false,
      "options": {
        "buildTarget": "worker-contact-submit:deploy"
      },
      "production": {
        "buildTarget": "worker-contact-submit:deploy:production"
      }
    },
    "semantic-release": {
      "executor": "@theunderscorer/nx-semantic-release:semantic-release",
      "options": {
        "github": true,
        "npm": false,
        "changelog": true,
        "tagFormat": "worker-contact-submit-v${VERSION}"
      }
    }
  },
  "tags": []
}
