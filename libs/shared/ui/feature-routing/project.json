{
  "name": "shared-ui-feature-routing",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/ui/feature-routing/src",
  "projectType": "library",
  "tags": ["domain:shared", "type:feature", "target:web"],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/ui/feature-routing",
        "tsConfig": "libs/shared/ui/feature-routing/tsconfig.lib.json",
        "project": "libs/shared/ui/feature-routing/package.json",
        "entryFile": "libs/shared/ui/feature-routing/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/shared/ui/feature-routing/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/shared/ui/feature-routing/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  }
}
