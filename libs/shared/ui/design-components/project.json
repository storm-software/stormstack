{
  "name": "shared-ui-design-components",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/ui/design-components/src",
  "projectType": "library",
  "generators": {
    "@nxext/stencil:component": {
      "style": "css"
    }
  },
  "tags": [],
  "targets": {
    "test": {
      "executor": "@nxext/stencil:test",
      "outputs": ["{options.outputPath}"],
      "options": {
        "projectType": "library",
        "tsConfig": "libs/shared/ui/design-components/tsconfig.lib.json",
        "configPath": "libs/shared/ui/design-components/stencil.config.ts",
        "outputPath": "dist/libs/shared/ui/design-components"
      }
    },
    "build": {
      "executor": "@nxext/stencil:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "projectType": "library",
        "tsConfig": "libs/shared/ui/design-components/tsconfig.lib.json",
        "configPath": "libs/shared/ui/design-components/stencil.config.ts",
        "outputPath": "dist/libs/shared/ui/design-components"
      },
      "configurations": {
        "production": {
          "dev": false,
          "prod": true
        }
      }
    },
    "serve": {
      "executor": "@nxext/stencil:serve",
      "outputs": ["{options.outputPath}"],
      "options": {
        "projectType": "library",
        "tsConfig": "libs/shared/ui/design-components/tsconfig.lib.json",
        "configPath": "libs/shared/ui/design-components/stencil.config.ts",
        "outputPath": "dist/libs/shared/ui/design-components"
      }
    },
    "e2e": {
      "executor": "@nxext/stencil:e2e",
      "outputs": ["{options.outputPath}"],
      "options": {
        "projectType": "library",
        "tsConfig": "libs/shared/ui/design-components/tsconfig.lib.json",
        "configPath": "libs/shared/ui/design-components/stencil.config.ts",
        "outputPath": "dist/libs/shared/ui/design-components"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": "libs/shared/ui/design-components/**/*.{ts,tsx}"
      }
    },
    "storybook": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "nx run shared-ui-design-components:serve",
          "nx run shared-ui-design-components:serve-storybook"
        ],
        "parallel": true
      }
    },
    "serve-storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/html",
        "port": 4400,
        "config": {
          "configFolder": "libs/shared/ui/design-components/.storybook"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "uiFramework": "@storybook/html",
        "outputPath": "dist/storybook/shared-ui-design-components",
        "config": {
          "configFolder": "libs/shared/ui/design-components/.storybook"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  }
}
