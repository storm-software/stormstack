{
  "name": "blog-engagement-ui-feature-reaction",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/blog/engagement/ui/feature-reaction/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/blog/engagement/ui/feature-reaction",
        "tsConfig": "libs/blog/engagement/ui/feature-reaction/tsconfig.lib.json",
        "project": "libs/blog/engagement/ui/feature-reaction/package.json",
        "entryFile": "libs/blog/engagement/ui/feature-reaction/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "swc",
        "assets": [
          {
            "glob": "libs/blog/engagement/ui/feature-reaction/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/blog/engagement/ui/feature-reaction/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/blog/engagement/ui/feature-reaction/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "semantic-release": {
      "executor": "@theunderscorer/nx-semantic-release:semantic-release",
      "options": {
        "github": true,
        "npm": false,
        "changelog": true,
        "tagFormat": "engagement-ui-feat-like-v${VERSION}"
      }
    }
  }
}
