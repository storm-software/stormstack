{
  "name": "core-feature-pdf",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/core/typescript/feature-pdf/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/core/typescript/feature-pdf/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/core/typescript/feature-pdf",
        "tsConfig": "libs/core/typescript/feature-pdf/tsconfig.lib.json",
        "project": "libs/core/typescript/feature-pdf/package.json",
        "entryFile": "libs/core/typescript/feature-pdf/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "libs/core/typescript/feature-pdf/rollup.config.js",
        "compiler": "swc",
        "assets": [
          {
            "glob": "libs/core/typescript/feature-pdf/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "semantic-release": {
      "executor": "@theunderscorer/nx-semantic-release:semantic-release",
      "options": {
        "github": true,
        "npm": false,
        "changelog": true,
        "tagFormat": "core-feature-pdf-v${VERSION}"
      }
    }
  }
}
