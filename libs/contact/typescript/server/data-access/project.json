{
  "name": "contact-server-data-access",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/contact/typescript/server/data-access/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/contact/typescript/server/data-access",
        "tsConfig": "libs/contact/typescript/server/data-access/tsconfig.lib.json",
        "packageJson": "libs/contact/typescript/server/data-access/package.json",
        "main": "libs/contact/typescript/server/data-access/src/index.ts",
        "assets": [
          "libs/contact/typescript/server/data-access/*.md"
        ]
      }
    },
    "prisma-init": {
      "command": "prisma init",
      "options": {
        "cwd": "libs/contact/typescript/server/data-access"
      }
    },
    "prisma-migrate": {
      "command": "prisma migrate dev",
      "options": {
        "cwd": "libs/contact/typescript/server/data-access"
      }
    },
    "generate": {
      "command": "prisma generate --data-proxy",
      "options": {
        "cwd": "libs/contact/typescript/server/data-access"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/contact/typescript/server/data-access/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/contact/typescript/server/data-access/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "semantic-release": {
      "executor": "@theunderscorer/nx-semantic-release:semantic-release",
      "options": {
        "github": true,
        "npm": false,
        "changelog": true,
        "tagFormat": "contact-server-data-access-v${VERSION}"
      }
    }
  },
  "tags": []
}
