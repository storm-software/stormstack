  "components": {
    "schemas": {
      "Contact": {
        "allOf": [
          {
            "$ref": "../../../shared/config/src/shared.api-spec.json#/components/schemas/RecordBase"
          },
          {
            "$ref": "#/components/schemas/ContactHeader"
          },
          {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ContactDetail"
            }
          }
        ],
        "title": "Contact",
        "description": "A full contact model including all of it's previously submitted details"
      },
      "ContactHeader": {
        "allOf": [
          {
            "properties": {
              "firstName": {
                "type": "string",
                "maxLength": 50
              },
              "lastName": {
                "type": "string",
                "maxLength": 50
              },
              "phoneNumber": {
                "type": "string",
                "maxLength": 15,
                "pattern": "^[\\+]?[(]?[0-9]{3}[)]?[-\\s\\.]?[0-9]{3}[-\\s\\.]?[0-9]{4,6}$"
              },
              "email": {
                "type": "string",
                "description": "The email address of the contact",
                "format": "email"
              },
              "isSubscribed": {
                "type": "boolean",
                "default": true
              }
            },
            "required": [
              "email",
              "reason",
              "isSubscribed"
            ]
          },
          {
            "$ref": "../../../shared/config/src/shared.api-spec.json#/components/schemas/Address"
          }
        ],
        "type": "object",
        "title": "ContactHeader",
        "description": "Contact data used to identify an individual and attach details too.",
        "x-stoplight": {
          "id": "4cf7f1cb259e4"
        }
      },
      "ContactDetail": {
        "type": "object",
        "title": "ContactDetail",
        "description": "Base data used to capture a contact request ",
        "properties": {
          "companyName": {
            "type": "string",
            "maxLength": 50
          },
          "title": {
            "type": "string",
            "maxLength": 50
          },
          "reason": {
            "type": "string",
            "example": "business",
            "description": "The type of contact request",
            "default": "business",
            "enum": [
              "business",
              "question",
              "other",
              "project",
              "interest",
              "subscription"
            ]
          },
          "details": {
            "type": "string",
            "maxLength": 2000
          },
          "url": {
            "type": "string",
            "format": "uri"
          }
        },
        "required": [
          "reason"
        ]
      }
    },
    "requestBodies": {
      "CreateContactRequest": {
        "content": {
          "application/json": {
            "schema": {
              "allOf": [
                {
                  "$ref": "#/components/schemas/ContactHeader"
                },
                {
                  "$ref": "#/components/schemas/ContactDetail"
                }
              ]
            }
          }
        },
        "description": ""
      }
    },
