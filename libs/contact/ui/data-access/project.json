{
  "name": "contact-ui-data-access",
  "sourceRoot": "libs/contact/ui/data-access/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/contact/ui/data-access/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "build": {
      "executor": "@nrwl/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/contact/ui/data-access",
        "tsConfig": "libs/contact/ui/data-access/tsconfig.lib.json",
        "project": "libs/contact/ui/data-access/package.json",
        "entryFile": "libs/contact/ui/data-access/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "swc",
        "assets": [
          {
            "glob": "libs/contact/ui/data-access/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/contact/ui/data-access/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "client-api-joi-sync": {
      "executor": "@open-system/executors-typescript:client-api-joi-sync",
      "options": {
        "prettierConfig": ".prettierrc",
        "specJsonFile": "libs/contact/config/src/contact.api-spec.json",
        "output": "libs/contact/ui/data-access/src/validate/contact.ts"
      }
    },
    "client-api-redux-sync": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx @rtk-query/codegen-openapi libs/contact/ui/data-access/src/apis/openapi-config.ts"
        ],
        "parallel": true
      }
    }
  }
}
