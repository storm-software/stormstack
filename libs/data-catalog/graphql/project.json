{
  "name": "data-catalog-graphql",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/data-catalog/graphql/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/data-catalog/graphql/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/data-catalog/graphql",
        "tsConfig": "libs/data-catalog/graphql/tsconfig.lib.json",
        "project": "libs/data-catalog/graphql/package.json",
        "entryFile": "libs/data-catalog/graphql/src/index.ts",
        "external": ["react", "react-dom", "react/jsx-runtime"],
        "rollupConfig": "@nx/react/plugins/bundle-rollup",
        "compiler": "swc",
        "assets": [
          {
            "glob": "libs/data-catalog/graphql/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    }
  }
}
