{
  "name": "portfolio-ui-feature-resume",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/portfolio/ui/feature-resume/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/portfolio/ui/feature-resume/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/portfolio/ui/feature-resume",
        "tsConfig": "libs/portfolio/ui/feature-resume/tsconfig.lib.json",
        "project": "libs/portfolio/ui/feature-resume/package.json",
        "entryFile": "libs/portfolio/ui/feature-resume/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "swc",
        "assets": [
          {
            "glob": "libs/portfolio/ui/feature-resume/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/portfolio/ui/feature-resume/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
