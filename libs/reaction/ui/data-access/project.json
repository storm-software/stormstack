{
  "name": "reaction-ui-data-access",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/reaction/ui/data-access/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/reaction/ui/data-access",
        "tsConfig": "libs/reaction/ui/data-access/tsconfig.lib.json",
        "project": "libs/reaction/ui/data-access/package.json",
        "entryFile": "libs/reaction/ui/data-access/src/index.ts",
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "assets": [
          {
            "glob": "libs/reaction/ui/data-access/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/reaction/ui/data-access/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/reaction/ui/data-access/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "client-redux-sync": {
      "executor": "@open-system/executors-typescript:client-redux-sync",
      "options": {
        "domainName": "reaction",
        "specJsonFile": "libs/reaction/config/src/reaction.api-spec.json",
        "generator": "open-system-typescript-redux"
      }
    },
    "semantic-release": {
      "executor": "@theunderscorer/nx-semantic-release:semantic-release",
      "options": {
        "github": true,
        "npm": false,
        "changelog": true,
        "tagFormat": "reaction-ui-data-access-v${VERSION}"
      }
    }
  }
}
