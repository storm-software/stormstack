{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "moduleResolution": "node",
    "types": ["node", "reflect-metadata"],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "target": "es2017",
    "module": "esnext",
    "lib": ["esnext", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "paths": {
      "@open-system/asyncapi-rabbitmq": ["tools/asyncapi/rabbitmq/index.ts"],
      "@open-system/contact-ui-data-access": [
        "libs/contact/ui/data-access/src/index.ts"
      ],
      "@open-system/contact-ui-data-access/*": [
        "libs/contact/ui/data-access/src/*"
      ],
      "@open-system/contact-ui-feature-form": [
        "libs/contact/ui/feature-form/src/index.ts"
      ],
      "@open-system/contact-ui-feature-form/*": [
        "libs/contact/ui/feature-form/src/*"
      ],
      "@open-system/contacts/config": ["libs/contacts/config/src/index.ts"],
      "@open-system/core-typescript-data-access": [
        "libs/core/typescript/data-access/src/index.ts"
      ],
      "@open-system/core-typescript-kafka": [
        "libs/core/typescript/kafka/src/index.ts"
      ],
      "@open-system/core-typescript-utilities": [
        "libs/core/typescript/utilities/src/index.ts"
      ],
      "@open-system/data-catalog-graphql": [
        "libs/data-catalog/graphql/src/index.ts"
      ],
      "@open-system/data-catalog-graphql/*": [
        "libs/data-catalog/graphql/src/*"
      ],
      "@open-system/design-system-components": [
        "design-system/components/src/index.ts"
      ],
      "@open-system/design-system-tokens": ["dist/design-system/tokens"],
      "@open-system/design-system-tokens/*": ["dist/design-system/tokens/*"],
      "@open-system/executors-docs": ["tools/executors/docs/index.ts"],
      "@open-system/executors-dotnet": ["tools/executors/dotnet/index.ts"],
      "@open-system/executors-typescript": [
        "tools/executors/typescript/index.ts"
      ],
      "@open-system/generators-dotnet": ["tools/generators/dotnet/index.ts"],
      "@open-system/generators-typescript": [
        "tools/generators/typescript/index.ts"
      ],
      "@open-system/portfolio-ui-feature-resume": [
        "libs/portfolio/ui/feature-resume/src/index.ts"
      ],
      "@open-system/portfolio-ui-feature-resume/*": [
        "libs/portfolio/ui/feature-resume/src/*"
      ],
      "@open-system/reaction-ui-data-access": [
        "libs/reaction/ui/data-access/src/index.ts"
      ],
      "@open-system/reaction-ui-data-access/*": [
        "libs/reaction/ui/data-access/src/*"
      ],
      "@open-system/reaction-ui-feature-like": [
        "libs/reaction/ui/src/index.ts"
      ],
      "@open-system/reaction-ui-feature-like/*": [
        "libs/reaction/ui/feature-like/src/*"
      ],
      "@open-system/reaction/config": ["libs/reaction/config/src/index.ts"],
      "@open-system/shared-config": ["libs/shared/config/src/index.ts"],
      "@open-system/shared-ui-components": [
        "libs/shared/ui/components/src/index.ts"
      ],
      "@open-system/shared-ui-components/*": [
        "libs/shared/ui/components/src/*"
      ],
      "@open-system/shared-ui-data-access": [
        "libs/shared/ui/data-access/src/index.ts"
      ],
      "@open-system/shared-ui-data-access/*": [
        "libs/shared/ui/data-access/src/*"
      ],
      "@open-system/shared-ui-feature-form": [
        "libs/shared/ui/feature-form/src/index.ts"
      ],
      "@open-system/shared-ui-feature-form/*": [
        "libs/shared/ui/feature-form/src/*"
      ],
      "@open-system/shared-ui-feature-layout": [
        "libs/shared/ui/feature-layout/src/index.ts"
      ],
      "@open-system/shared-ui-feature-layout/*": [
        "libs/shared/ui/feature-layout/src/*"
      ],
      "@open-system/shared-ui-feature-notifications": [
        "libs/shared/ui/feature-notifications/src/index.ts"
      ],
      "@open-system/shared-ui-feature-notifications/*": [
        "libs/shared/ui/feature-notifications/src/*"
      ],
      "@open-system/shared-ui-feature-pdf": [
        "libs/shared/ui/feature-pdf/src/index.ts"
      ],
      "@open-system/shared-ui-feature-pdf/*": [
        "libs/shared/ui/feature-pdf/src/*"
      ],
      "@open-system/static-message-config": [
        "libs/static/message/config/src/index.ts"
      ],
      "@open-system/user-ui-data-access": [
        "libs/user/ui/data-access/src/index.ts"
      ],
      "@open-system/user/config": ["libs/user/config/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"],
  "references": [
    {
      "path": "./.storybook/tsconfig.json"
    }
  ]
}
