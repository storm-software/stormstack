  "nextBundleAnalysis": {
    "buildOutputDirectory": "dist/apps/web/shell/.next"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  },
  "bundlewatch": {
    "files": [
      {
        "path": "dist/*/*.js",
        "maxSize": "200kB"
      }
    ],
    "ci": {
      "trackBranches": [
        "main",
        "alpha",
        "beta"
      ]
    }
  },
