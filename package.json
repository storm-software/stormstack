  "nextBundleAnalysis": {
    "buildOutputDirectory": "dist/apps/web/shell/.next"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": [
      "pnpm nx format"
    ],
    "**/*.{js,cjs,mjs,ts,tsx}": [
      "pnpm eslint --fix"
    ],
    "**/*.{js,jsx,ts,tsx,json,css,scss,md,mdx,yml,yaml,graphql,html}": [
      "pnpm prettier --write"
    ],
    "**/*.{js,jsx,ts,tsx,md,mdx}": [
      "pnpm lint:language"
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "pnpm typecheck"
    ],
    "**/README.md": [
      "pnpm exec nx generate @open-system/tools-generators-typescript:repo-readme-format --templatePath=\"libs/core/config/src/readme-templates\" --no-interactive"
    ]
  },
  "relay": {
    "src": "./",
    "artifactDirectory": "./apps/web/shell/__generated__/relay",
    "language": "typescript",
    "schema": "./.wundergraph/generated/wundergraph.schema.graphql",
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**",
      "**/.wundergraph/generated/**"
    ],
    "persistConfig": {
      "file": "./.wundergraph/operations/relay/persisted.json"
    },
    "eagerEsModules": true
  },
  "bundlewatch": {
    "files": [
      {
        "path": "dist/*/*.js",
        "maxSize": "200kB"
      }
    ],
    "ci": {
      "trackBranches": [
        "main",
        "alpha",
        "beta"
      ]
    }
  },
  "pnpm": {
    "patchedDependencies": {
      "@nx/js@16.7.0": "patches/@nx__js@16.7.0.patch",
