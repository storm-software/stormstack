    "@nx-plus/docusaurus": "^14.1.0",
    "@nxext/stencil": "^14.0.6",
    "@stencil/core": "2.17.1",
    "@stencil/postcss": "^2.1.0",
    "@stencil/react-output-target": "^0.3.1",
    "@storybook/addon-a11y": "^6.5.13",
