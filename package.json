  "description": "ðŸ§ª This repository contains modern and scalable web application code, additional utility applications/tools, and various libraries ðŸ§ª",
  "author": "Pat Sullivan <Patrick.Joseph.Sullivan@ProtonMail.com>",
  "homepage": "https://sullivanpj.github.io/open-system/",
  "repository": {
    "type": "github",
    "url": "https://github.com/sullivanpj/open-system.git"
  },
  "bugs": {
    "url": "https://github.com/sullivanpj/open-system/issues"
  },
  "keywords": [
    "pat",
    "patrick",
    "sullivan",
    "sullivanpj",
    "open-system",
    "open",
    "system"
  ],
  "workspaces": [
    "apps/*",
    "libs/*",
    "design-system/*",
    "tools/*",
    "docs/*"
  ],
  "scripts": {
    "nx": "nx",
    "prepare:husky": "is-ci || husky install",
    "prepare": "npm run prepare:husky && npx patch-package -y",
    "clean:apps": "rimraf dist/apps",
    "clean:libs": "rimraf dist/libs",
    "clean:tools": "rimraf dist/tools",
    "clean": "rimraf dist",
    "build:tools-executors-typescript-cloudflare": "npx nx run tools-executors-typescript:build:cloudflare-worker-build && npx nx run tools-executors-typescript:build:cloudflare-worker-serve && npx nx run tools-executors-typescript:build:cloudflare-worker-deploy",
    "build:tools-executors-typescript": "npx nx run tools-executors-typescript:build:client-api-sync && npx nx run tools-executors-typescript:build:design-tokens-build && npm run build:tools-executors-typescript-cloudflare",
    "build:tools-generators-typescript-cloudflare": "npx nx run tools-generators-typescript:build:cloudflare-worker",
    "build:tools-generators-typescript": "npx nx run tools-generators-typescript:build:client-api && npm run build:tools-generators-typescript-cloudflare",
    "build:tools-executors-dotnet": "npx nx run tools-executors-dotnet:build:async-api-sync && npx nx run tools-executors-dotnet:build:server-api-sync && npx nx run tools-executors-dotnet:build:build",
    "build:tools-generators-dotnet": "npx nx run tools-generators-dotnet:build:async-api && npx nx run tools-generators-dotnet:build:server-api && npx nx run tools-generators-dotnet:build:restore",
    "build:tools-executors-docs": "npx nx run tools-executors-docs:build:event-catalog-build",
    "build:tools-typescript": "npm run build:tools-executors-typescript && npm run build:tools-generators-typescript",
    "build:tools-dotnet": "npm run build:tools-executors-dotnet && npm run build:tools-generators-dotnet",
    "build:tools": "cross-env FORCE_COLOR=1 npm run build:tools-typescript && npm run build:tools-dotnet && npm run build:tools-executors-docs",
    "build:tools-ci": "npx nx run tools-executors-typescript:build:design-tokens-build && npx nx run design-system-tokens:build:open-system",
    "build:web-deploy": "npm run build:tools-ci && npx nx run web-shell:build:production",
    "build:storybook": "cross-env FORCE_COLOR=1 storybook build -s public",
    "build:wundergraph": "wunderctl generate",
    "build": "npm run build:wundergraph && relay-compiler && nx run-many -t build --all --parallel 4",
    "lint:spellcheck": "cross-env FORCE_COLOR=1 cspell --no-progress --relative --dot --gitignore",
    "lint:language": "cross-env FORCE_COLOR=1 alex .",
    "lint:js": "cross-env FORCE_COLOR=1 eslint --cache --report-unused-disable-directives \"**/*.{js,jsx,ts,tsx}\"",
    "lint": "nx lint && npm run lint:spellcheck && npm run lint:language && npm run lint:js",
    "analyze": "cross-env ANALYZE=true npm run build",
    "test:storybook": "cross-env FORCE_COLOR=1 test-storybook",
    "test": "nx test",
    "storybook": "cross-env FORCE_COLOR=1 storybook dev -p 6006",
    "start": "nx serve",
    "format": "nx format",
    "relay": "relay-compiler --watch",
    "wundergraph": "wunderctl up --debug",
    "add:contributors": "all-contributors add",
    "generate:contributors": "all-contributors generate",
    "add:decisions": "npx log4brains adr new",
    "preview:decisions": "npx log4brains preview"
  },
  "relay": {
    "src": "./",
    "artifactDirectory": "./apps/web/shell/__generated__/relay",
    "language": "typescript",
    "schema": "./.wundergraph/generated/wundergraph.schema.graphql",
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**",
      "**/.wundergraph/generated/**"
    ],
    "persistConfig": {
      "file": "./.wundergraph/operations/relay/persisted.json"
    },
    "eagerEsModules": true
  },
