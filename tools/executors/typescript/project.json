{
  "name": "tools-executors-typescript",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "tools/executors/typescript",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "tools/executors/typescript/**/*.ts",
          "tools/executors/typescript/executors.json",
          "tools/executors/typescript/package.json"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/tools/executors/typescript",
        "tsConfig": "tools/executors/typescript/tsconfig.json",
        "project": "tools/executors/typescript/package.json",
        "main": "tools/executors/typescript/index.ts",
        "rollupConfig": "tools/executors/typescript/rollup.config.js",
        "external": "all",
        "deleteOutputPath": false,
        "assets": [
          {
            "input": "./tools/executors/typescript",
            "glob": "**/*!(design-token-parsers)/**/*.md",
            "output": "."
          },
          {
            "input": "./tools/executors/typescript",
            "glob": "**/schema.json",
            "output": "."
          },
          {
            "input": "./tools/executors/typescript",
            "glob": "executors.json",
            "output": "."
          },
          {
            "input": "./tools/executors/typescript",
            "glob": "executors.json",
            "output": "."
          },
          {
            "input": "",
            "glob": "CHANGELOG.md",
            "output": "/"
          },
          {
            "input": "",
            "glob": "LICENSE",
            "output": "/"
          },
          {
            "input": "./tools/executors/typescript",
            "glob": "migrations.json",
            "output": "."
          }
        ]
      },
      "configurations": {
        "cloudflare-worker-build": {
          "outputPath": "dist/tools/executors/typescript/cloudflare-worker-build",
          "main": "tools/executors/typescript/cloudflare-worker-build/impl.ts",
          "deleteOutputPath": true,
          "assets": [
            {
              "input": "tools/executors/typescript/cloudflare-worker-build",
              "glob": "schema.json",
              "output": "."
            }
          ]
        },
        "cloudflare-worker-deploy": {
          "outputPath": "dist/tools/executors/typescript/cloudflare-worker-deploy",
          "main": "tools/executors/typescript/cloudflare-worker-deploy/impl.ts",
          "deleteOutputPath": true,
          "assets": [
            {
              "input": "tools/executors/typescript/cloudflare-worker-deploy",
              "glob": "schema.json",
              "output": "."
            }
          ]
        },
        "cloudflare-worker-serve": {
          "outputPath": "dist/tools/executors/typescript/cloudflare-worker-serve",
          "main": "tools/executors/typescript/cloudflare-worker-serve/impl.ts",
          "deleteOutputPath": true,
          "assets": [
            {
              "input": "tools/executors/typescript/cloudflare-worker-serve",
              "glob": "schema.json",
              "output": "."
            }
          ]
        },
        "design-tokens-build": {
          "outputPath": "dist/tools/executors/typescript/design-tokens-build",
          "main": "tools/executors/typescript/design-tokens-build/impl.ts",
          "deleteOutputPath": true,
          "assets": [
            {
              "input": "tools/executors/typescript/design-tokens-build",
              "glob": "schema.json",
              "output": "."
            }
          ]
        },
        "design-components-clean": {
          "outputPath": "dist/tools/executors/typescript/design-components-clean",
          "main": "tools/executors/typescript/design-components-clean/impl.ts",
          "deleteOutputPath": true,
          "assets": [
            {
              "input": "tools/executors/typescript/design-components-clean",
              "glob": "schema.json",
              "output": "."
            }
          ]
        },
        "client-api-sync": {
          "outputPath": "dist/tools/executors/typescript/client-api-sync",
          "main": "tools/executors/typescript/client-api-sync/impl.ts",
          "deleteOutputPath": true,
          "assets": [
            {
              "input": "tools/executors/typescript/client-api-sync",
              "glob": "schema.json",
              "output": "."
            }
          ]
        },
        "client-gql-sync": {
          "outputPath": "dist/tools/executors/typescript/client-gql-sync",
          "main": "tools/executors/typescript/client-gql-sync/impl.ts",
          "deleteOutputPath": true,
          "assets": [
            {
              "input": "tools/executors/typescript/client-gql-sync",
              "glob": "schema.json",
              "output": "."
            }
          ]
        },
        "event-library": {
          "outputPath": "dist/tools/executors/typescript/event-library",
          "main": "tools/executors/typescript/event-library/impl.ts",
          "deleteOutputPath": true,
          "assets": [
            {
              "input": "tools/executors/typescript/event-library",
              "glob": "schema.json",
              "output": "."
            }
          ]
        }
      }
    },
    "semantic-release": {
      "executor": "@theunderscorer/nx-semantic-release:semantic-release",
      "options": {
        "github": true,
        "npm": false,
        "changelog": true,
        "tagFormat": "tools-executors-typescript-v${VERSION}"
      }
    }
  },
  "tags": [],
  "implicitDependencies": [
    "core-shared-utilities",
    "core-server-utilities",
    "core-server-cloudflare",
    "tools-async-api-kafka"
  ]
}
