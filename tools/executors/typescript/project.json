            },
            {
              "input": "",
              "glob": "LICENSE",
              "output": "/"
            },
            {
              "input": "tools/executors/typescript/design-tokens-build",
              "glob": "schema.json",
              "output": "."
            }
          ]
        },
        "client-api-sync": {
