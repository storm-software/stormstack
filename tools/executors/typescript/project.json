          "assets": [
            {
              "input": "./tools/executors/typescript",
              "glob": "**/*!(design-gql-sync)/**/*.md",
              "output": "."
            },
            {
              "input": "",
              "glob": "CHANGELOG.md",
              "output": "/"
            },
            {
              "input": "",
              "glob": "LICENSE",
              "output": "/"
            },
            {
              "input": "tools/executors/typescript/client-gql-sync",
              "glob": "schema.json",
              "output": "."
            }
          ]
        }
      }
    },
    "semantic-release": {
      "executor": "@theunderscorer/nx-semantic-release:semantic-release",
      "options": {
        "github": true,
        "npm": false,
        "changelog": true,
        "tagFormat": "tools-executors-typescript-v${VERSION}"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["core-utilities"]
}
