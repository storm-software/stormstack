{
  "name": "tools-executors-typescript",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "tools/executors/typescript",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "tools/executors/typescript/**/*.ts",
          "tools/executors/typescript/executors.json",
          "tools/executors/typescript/package.json"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/tools/executors/typescript",
        "tsConfig": "tools/executors/typescript/tsconfig.json",
        "project": "tools/executors/typescript/package.json",
        "main": "tools/executors/typescript/index.ts",
        "deleteOutputPath": false,
        "updateBuildableProjectDepsInPackageJson": true,
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "assets": [
          {
            "input": "./tools/executors/typescript",
            "glob": "**/*!(design-token-parsers)/**/*.md",
            "output": "."
          },
          {
            "input": "./tools/executors/typescript",
            "glob": "**/schema.json",
            "output": "."
          },
          {
            "input": "./tools/executors/typescript",
            "glob": "executors.json",
            "output": "."
          },
          {
            "input": "./tools/executors/typescript",
            "glob": "executors.json",
            "output": "."
          },
          {
            "input": "",
            "glob": "CHANGELOG.md",
            "output": "/"
          },
          {
            "input": "",
            "glob": "LICENSE",
            "output": "/"
          },
          {
            "input": "./tools/executors/typescript",
            "glob": "migrations.json",
            "output": "."
          }
        ]
      },
      "configurations": {
        "cloudflare-worker-build": {
          "outputPath": "dist/tools/executors/typescript/cloudflare-worker-build",
          "rollupConfig": "tools/executors/typescript/cloudflare-worker-build/rollup.config.js",
          "assets": [
            {
              "input": "./tools/executors/typescript",
              "glob": "**/*!(design-token-parsers)/**/*.md",
              "output": "."
            },
            {
              "input": "",
              "glob": "CHANGELOG.md",
              "output": "/"
            },
            {
              "input": "",
              "glob": "LICENSE",
              "output": "/"
            },
            {
              "input": "tools/executors/typescript/cloudflare-worker-build",
              "glob": "schema.json",
              "output": "."
            }
          ]
        },
        "cloudflare-worker-deploy": {
          "outputPath": "dist/tools/executors/typescript/cloudflare-worker-deploy",
          "rollupConfig": "tools/executors/typescript/cloudflare-worker-deploy/rollup.config.js",
          "assets": [
            {
              "input": "./tools/executors/typescript",
              "glob": "**/*!(design-token-parsers)/**/*.md",
              "output": "."
            },
            {
              "input": "",
              "glob": "CHANGELOG.md",
              "output": "/"
            },
            {
              "input": "",
              "glob": "LICENSE",
              "output": "/"
            },
            {
              "input": "tools/executors/typescript/cloudflare-worker-deploy",
              "glob": "schema.json",
              "output": "."
            }
          ]
        },
        "cloudflare-worker-serve": {
          "outputPath": "dist/tools/executors/typescript/cloudflare-worker-serve",
          "rollupConfig": "tools/executors/typescript/cloudflare-worker-serve/rollup.config.js",
          "assets": [
            {
              "input": "./tools/executors/typescript",
              "glob": "**/*!(design-token-parsers)/**/*.md",
              "output": "."
            },
            {
              "input": "",
              "glob": "CHANGELOG.md",
              "output": "/"
            },
            {
              "input": "",
              "glob": "LICENSE",
              "output": "/"
            },
            {
              "input": "tools/executors/typescript/cloudflare-worker-serve",
