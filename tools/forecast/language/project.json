{
  "name": "tools-forecast-language",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "tools/forecast/language/src",
  "projectType": "library",
  "targets": {
    "compile": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/tools/forecast/language",
        "main": "tools/forecast/language/src/main.ts",
        "additionalEntryPoints": [
          "tools/forecast/language/src/ast.ts",
          "tools/forecast/language/src/forecast-module.ts",
          "tools/forecast/language/src/__generated__/grammar.ts",
          "tools/forecast/language/src/main-browser.ts"
        ],
        "project": "tools/forecast/language/package.json",
        "tsConfig": "tools/forecast/language/tsconfig.json",
        "platform": "node",
        "deleteOutputPath": true,
        "metafile": true,
        "bundle": true,
        "sourcemap": true,
        "minify": false,
        "skipTypeCheck": true,
        "format": ["esm", "cjs"],
        "external": [".prisma/client/index"],
        "assets": [
          {
            "input": "tools/forecast/language",
            "glob": "README.md",
            "output": "."
          },
          {
            "input": ".",
            "glob": "LICENSE",
            "output": "."
          },
          {
            "input": "assets/favicons/light",
            "glob": "icon.png",
            "output": "assets/light"
          },
          {
            "input": "assets/favicons/dark",
            "glob": "icon.png",
            "output": "assets/dark"
          },
          {
            "input": "tools/forecast/language/syntaxes",
            "glob": "*.*",
            "output": "syntaxes"
          },
          {
            "input": "tools/forecast/language/examples",
            "glob": "*.*",
            "output": "examples"
          }
        ]
      }
    },
    "compile-extension": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/tools/forecast/extension",
        "main": "tools/forecast/language/src/extension/main.ts",
        "additionalEntryPoints": [
          "tools/forecast/language/src/main.ts",
          "tools/forecast/language/src/main-browser.ts"
        ],
        "project": "tools/forecast/language/package.json",
        "tsConfig": "tools/forecast/language/tsconfig.json",
        "external": ["vscode"],
        "platform": "node",
        "metafile": true,
        "bundle": true,
        "sourcemap": false,
        "minify": true,
        "skipTypeCheck": true,
        "deleteOutputPath": true,
        "assets": [
          {
            "input": "tools/forecast/language",
            "glob": "README.md",
            "output": "."
          },
          {
            "input": ".",
            "glob": "LICENSE",
            "output": "."
          },
          {
            "input": "tools/forecast/language/src/res",
            "glob": "*.*",
            "output": "res"
          },
          {
            "input": "assets/favicons/light",
            "glob": "icon.png",
            "output": "assets/light"
          },
          {
            "input": "assets/favicons/dark",
            "glob": "icon.png",
            "output": "assets/dark"
          },
          {
            "input": "tools/forecast/language/syntaxes",
            "glob": "*.*",
            "output": "syntaxes"
          },
          {
            "input": "tools/forecast/language",
            "glob": "language-configuration.json",
            "output": "."
          },
          {
            "input": "tools/forecast/language/examples",
            "glob": "*.*",
            "output": "examples"
          }
        ]
      }
    },
    "langium": {
      "executor": "nx:run-commands",
      "outputs": ["{options.outputPath}"],
      "outputPath": "tools/forecast/language",
      "defaultConfiguration": "default",
      "options": {
        "commands": ["npx langium generate"],
        "cwd": "tools/forecast/language"
      },
      "configurations": {
        "default": {
          "options": {
            "commands": ["npx langium generate"],
            "cwd": "tools/forecast/language"
          }
        },
        "watch": {
          "options": {
            "commands": ["npx langium generate --watch"],
            "cwd": "tools/forecast/language",
            "watch": true,
            "parallel": false
          }
        }
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{options.outputPath}"],
      "outputPath": "tools/forecast/language",
      "defaultConfiguration": "default",
      "options": {
        "commands": [
          "pnpm nx run tools-forecast-language:langium:default",
          "pnpm nx run tools-forecast-language:compile"
        ]
      },
      "configurations": {
        "default": {
          "options": {
            "commands": [
              "pnpm nx run tools-forecast-language:langium:default",
              "pnpm nx run tools-forecast-language:compile"
            ]
          }
        },
        "watch": {
          "options": {
            "commands": [
              "pnpm nx run tools-forecast-language:langium:watch",
              "pnpm nx run tools-forecast-language:compile"
            ],
            "watch": true,
            "parallel": false
          }
        }
      }
    },
    "build-extension": {
      "executor": "nx:run-commands",
      "outputs": ["{options.outputPath}"],
      "outputPath": "tools/forecast/language",
      "defaultConfiguration": "default",
      "options": {
        "commands": [
          "pnpm nx run tools-forecast-language:langium:default",
          "pnpm nx run tools-forecast-language:compile-extension"
        ]
      },
      "configurations": {
        "default": {
          "options": {
            "commands": [
              "pnpm nx run tools-forecast-language:langium:default",
              "pnpm nx run tools-forecast-language:compile-extension"
            ]
          }
        },
        "watch": {
          "options": {
            "commands": [
              "pnpm nx run tools-forecast-language:langium:watch",
              "pnpm nx run tools-forecast-language:compile-extension"
            ],
            "watch": true,
            "parallel": false
          }
        }
      }
    },
    "pack": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "pnpm nx run tools-forecast-language:build-extension",
          "pnpm pack \"./tools/forecast/language\" --pack-destination \"./dist/tools/forecast/extension\""
        ]
      }
    },
    "publish-dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "pnpm nx run tools-forecast-language:build-extension",
          "cd dist/tools/forecast/extension",
          "pnpm publish --registry http://localhost:4873"
        ]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "pnpm nx run tools-forecast-language:build-extension",
          "pnpm vsce package --no-dependencies",
          "pnpm vsce publish --no-dependencies"
        ]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "tools/storm/language/**/*.ts",
          "tools/storm/language/generators.json",
          "tools/storm/language/package.json"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "tools/storm/language/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "readme-format": {
      "executor": "@stormstack/tools-executors-docs:readme-format",
      "outputs": ["{options.outputFile}"],
      "options": {
        "outputPath": "tools/forecast/language",
        "inputFile": "tools/forecast/language/README.md",
        "templatePath": "libs/core/config/src/readme-templates"
      }
    },
    "semantic-release": {
      "executor": "@theunderscorer/nx-semantic-release:semantic-release",
      "options": {
        "github": true,
        "npm": false,
        "changelog": true,
        "tagFormat": "tools-forecast-language-v${VERSION}"
      }
    }
  },
  "tags": ["scope:tools", "platform:server"]
}
