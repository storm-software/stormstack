{
  "name": "@open-system/tools-storm-schema",
  "version": "0.0.1",
  "contributes": {
    "languages": [
      {
        "id": "storm",
        "aliases": [
          "Storm Model",
          "storm"
        ],
        "extensions": [
          ".storm"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./assets/logo.png",
          "dark": "./assets/logo.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "storm",
        "scopeName": "source.storm",
        "path": "./syntaxes/storm.tmLanguage.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:storm"
  ],
  "bin": {
    "storm": "bin/cli"
  },
  "devDependencies": {
    "@prisma/internals": "^5.1.1",
    "@types/vscode": "^1.81.0"
  },
  "dependencies": {
    "@vscode/vsce": "^2.20.1",
    "chalk": "4.1.0",
    "langium": "1.2.1",
    "langium-cli": "1.2.1",
    "ora": "^7.0.1",
    "path": "^0.12.7",
    "pluralize": "^8.0.0",
    "semver": "^7.5.4",
    "ts-morph": "^19.0.0",
    "ts-pattern": "^5.0.5",
    "vscode": "^1.1.37",
    "vscode-jsonrpc": "^8.1.0",
    "vscode-languageclient": "^8.1.0",
    "vscode-languageserver": "^8.1.0",
    "vscode-languageserver-textdocument": "^1.0.8",
    "vscode-uri": "^3.0.7"
  }
}
