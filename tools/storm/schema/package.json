{
  "name": "@open-system/tools-storm-schema",
  "version": "0.0.1",
  "keywords": [
    "fullstack",
    "react",
    "typescript",
    "data modeling"
  ],
  "preview": false,
  "icon": "asset/logo.png",
  "publishConfig": {
    "directory": "dist/tools/storm/schema",
    "linkDirectory": true
  },
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "storm",
        "aliases": [
          "Storm Model",
          "storm"
        ],
        "extensions": [
          ".storm"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./asset/logo.png",
          "dark": "./asset/logo.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "storm",
        "scopeName": "source.storm",
        "path": "./syntaxes/storm.tmLanguage.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:storm"
  ],
  "bin": {
    "storm": "bin/cli"
  }
}
