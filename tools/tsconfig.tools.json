{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "../dist/out-tsc",
    "rootDir": "../",
    "module": "commonjs",
    "target": "es5",
    "allowJs": true,
    "strict": false,
    "noImplicitOverride": false,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": false,
    "noPropertyAccessFromIndexSignature": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
